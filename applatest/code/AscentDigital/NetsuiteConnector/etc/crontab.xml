<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">

        <job instance="AscentDigital\NetsuiteConnector\Cron\Order\GetOrder" method="execute" name="get_so_number_from_netsuite_cron">
            <schedule>*/15 * * * *</schedule>
        </job>
        <job instance="AscentDigital\NetsuiteConnector\Cron\Customer\CustomerSpecificPrices" method="execute" name="get_customer_specific_prices_cron">
            <schedule>*/15 * * * *</schedule>
        </job>
        <job instance="AscentDigital\NetsuiteConnector\Cron\Order\Order" method="execute" name="get_order_cron">
            <schedule>*/15 * * * *</schedule>
        </job>
        <job instance="AscentDigital\NetsuiteConnector\Cron\Customer\CustomerSpecificTerms" method="execute" name="get_customer_specific_terms_cron">
            <schedule>0 */6 * * *</schedule>
        </job>
        <job instance="AscentDigital\NetsuiteConnector\Cron\Shipping\Shipping" method="execute" name="get_shipping_cron">
            <schedule>*/15 * * * *</schedule>
        </job>
       
         <job  instance="AscentDigital\NetsuiteConnector\Cron\AdvanceExchange\AdvanceExchangeCron" method="execute" name="advance_exhange_records_cron">
            <schedule>0 */4 * * *</schedule>
        </job>
        <job  instance="AscentDigital\NetsuiteConnector\Cron\Locations\Locations" method="execute" name="get_locations_cron">
            <schedule>0 */5 * * *</schedule>
        </job>
         <job  instance="AscentDigital\NetsuiteConnector\Cron\Item\DeletedProduct" method="execute" name="deleted_products_cron">
            <schedule>0 */24 * * *</schedule>
        </job>
         <job  instance="AscentDigital\NetsuiteConnector\Cron\Item\UpdateProducts" method="execute" name="update_products_cron">
            <schedule>*/15 * * * *</schedule>
        </job>
        
         <job  instance="AscentDigital\NetsuiteConnector\Cron\Order\GetAllOrder" method="execute" name="get_all_orders_cron">
            <schedule>*/15 * * * *</schedule>
        </job>
         <job  instance="AscentDigital\NetsuiteConnector\Cron\TruncateNSCrons" method="execute" name="truncate_ns_cron">
            <schedule>0 */24 * * *</schedule>
        </job>
         <job  instance="AscentDigital\NetsuiteConnector\Cron\Customer\CSMPhone" method="execute" name="csmphone_ns_cron">
            <schedule>0 */24 * * *</schedule>
        </job>
         <!-- <job  instance="AscentDigital\NetsuiteConnector\Cron\Item\UpdateProductsMobility" method="execute" name="update_products_mobility_cron">
            <schedule>*/5 * * * *</schedule>
        </job>
         <job  instance="AscentDigital\NetsuiteConnector\Cron\Item\UpdateProductsAriba" method="execute" name="update_products_ariba_cron">
            <schedule>*/5 * * * *</schedule>
        </job> -->
        
    </group>
</config>
