    <body>
        <referenceContainer name="content">
            <block class="AscentDigital\Reports\Block\Reports\FirstNet\DemoOrders" name="orders.checkout.for.orders.report" template="AscentDigital_Reports::reports/firstnet/demo_orders.phtml" cacheable="false"/>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="account/link/back.phtml" cacheable="false"/>
			<block class="AscentDigital\Reports\Block\Reports\FirstNet\Graph\DemoOrders" name="demo-checkout-orders-report-graph" template="Amasty_ReportBuilder::demo_orders_graph.phtml" />
        </referenceContainer>
    </body>
</page>
