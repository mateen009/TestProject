<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="cminds_oapm_cleaning" instance="Cminds\Oapm\Model\Cron" method="clean">
            <config_path>crontab/default/jobs/cminds_oapm_cleaning/schedule/cron_expr</config_path>
        </job>
        <job name="cminds_oapm_reminders" instance="Cminds\Oapm\Model\Cron" method="processReminders">
            <schedule>* */1 * * *</schedule>
        </job>
    </group>
</config>
