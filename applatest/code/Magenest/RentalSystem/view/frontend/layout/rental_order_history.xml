<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <!-- <script src="Magenest_RentalSystem::js/calendar.js"></script> -->
        <css src="Magenest_RentalSystem::css/fullcalendar.css" />
        <script src="Magenest_RentalSystem::js/custom.js"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magenest\RentalSystem\Block\Order\History" name="rental.order.history" cacheable="false">
                <container name="rental.order.history.info" as="info" label="Rents History Info"/>
            </block>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="account/link/back.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
