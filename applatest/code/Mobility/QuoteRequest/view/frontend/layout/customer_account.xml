<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Mobility\QuoteRequest\Block\View\Element\Html\Link\QuoteApprovals"
                    name="customer-account-navigation-quote-request-link">
                <arguments>
                    <argument name="path" xsi:type="string">quote/account/request</argument>
                    <argument name="label" xsi:type="string" translate="true">My Quote Requests</argument>
                    <argument name="sortOrder" xsi:type="number">220</argument>
                </arguments>
            </block>
            <block class="Mobility\QuoteRequest\Block\View\Element\Html\Link\OrderApprovals"
                    before="-"
                    name="customer-account-navigation-order-approval-link">
                <arguments>
                    <argument name="path" xsi:type="string">quote/account/orderapprovals</argument>
                    <argument name="label" xsi:type="string" translate="true">My Orders Approvals</argument>
                    <argument name="sortOrder" xsi:type="number">250</argument>
                </arguments>
            </block>
            
        </referenceBlock>
    </body>
</page>
