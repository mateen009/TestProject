<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Mobility\SalesTeamManagement\Block\View\Element\Html\Link\SalesRep"
                    name="customer-account-navigation-salesrep-list-link">
                <arguments>
                    <argument name="path" xsi:type="string">customer/salesrep/index</argument>
                    <argument name="label" xsi:type="string" translate="true">Manage Sales Rep</argument>
                    <argument name="sortOrder" xsi:type="number">240</argument>
                </arguments>
            </block>
            <block class="Mobility\SalesTeamManagement\Block\View\Element\Html\Link\SalesManager"
                    name="customer-account-navigation-salesmanager-list-link">
                <arguments>
                    <argument name="path" xsi:type="string">customer/salesmanager/index</argument>
                    <argument name="label" xsi:type="string" translate="true">Manage Sales Manager</argument>
                    <argument name="sortOrder" xsi:type="number">240</argument>
                </arguments>
            </block>
        </referenceBlock>
        
    </body>
</page>
