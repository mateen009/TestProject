<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Mobility\SalesTeamManagement\Block\Customer\SalesRep\AddNewSalesManager" name="mobility.customer.salesmanager.new" template="Mobility_SalesTeamManagement::customer/salesrep/newsalesmanager.phtml" cacheable="false">
                <container name="mobility.customer.salesmanager.new.info" as="info" label="Manage Sales Manager Info"/>
            </block>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="account/link/back.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
