<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Orders\Data\Block\Order\BackOrderHistory" name="mobility.customer.backorder.list" template="Orders_Data::order/backorder.phtml" cacheable="false">
                <container name="mobility.customer.backorder.list.info" as="info" label="Manage Due Back Order Info"/>
            </block>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="account/link/back.phtml" cacheable="false"/>
            <block class="Orders\Data\Block\Graph\DueBackOrders" name="due-back-report-graph" template="Amasty_ReportBuilder::due_back_orders_graph.phtml" />
        </referenceContainer>
    </body>
</page>


