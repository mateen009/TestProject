<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
<table name="ordered_csid" resource="default" engine="innodb" comment="Ordered CSID">
		<column xsi:type="int" name="id" padding="10" unsigned="false" nullable="false" identity="true" comment="ID" />
		<column xsi:type="varchar" length="255" name="csid" nullable="false" comment="CSID" />
		<column xsi:type="varchar" length="255" name="order_id" nullable="false" comment="Order Id" />
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="id" />
		</constraint>
        <constraint xsi:type="unique" referenceId="ORDERED_CSID_CSID">
			<column name="csid" />
		</constraint>
	</table>
</schema>