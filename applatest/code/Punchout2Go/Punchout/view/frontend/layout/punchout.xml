<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="https://connect.punchout2go.com/jslib/lib/basic/basic.css" src_type="url" />
    </head>
    <body>
        <referenceBlock name="header.links">
            <block class="Magento\Framework\View\Element\Template" name="punchout-close" template="Punchout2Go_Punchout::punchout_close.phtml" after="-"/>
        </referenceBlock>
        <referenceContainer name="footer">
            <block class="Magento\Framework\View\Element\Template" name="punchout.session.js" template="Punchout2Go_Punchout::js/punchout_config.phtml"/>
        </referenceContainer>
    </body>
</page>
